#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/build/buildroot/output/cvitek_CV181X_glibc_riscv64/build/nanokvm-server-4af32fc7032b9dde2f3efd4d917837eeca015a87/test/node_modules/.pnpm/nanoid@3.3.8/node_modules/nanoid/bin/node_modules:/build/buildroot/output/cvitek_CV181X_glibc_riscv64/build/nanokvm-server-4af32fc7032b9dde2f3efd4d917837eeca015a87/test/node_modules/.pnpm/nanoid@3.3.8/node_modules/nanoid/node_modules:/build/buildroot/output/cvitek_CV181X_glibc_riscv64/build/nanokvm-server-4af32fc7032b9dde2f3efd4d917837eeca015a87/test/node_modules/.pnpm/nanoid@3.3.8/node_modules:/build/buildroot/output/cvitek_CV181X_glibc_riscv64/build/nanokvm-server-4af32fc7032b9dde2f3efd4d917837eeca015a87/test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/build/buildroot/output/cvitek_CV181X_glibc_riscv64/build/nanokvm-server-4af32fc7032b9dde2f3efd4d917837eeca015a87/test/node_modules/.pnpm/nanoid@3.3.8/node_modules/nanoid/bin/node_modules:/build/buildroot/output/cvitek_CV181X_glibc_riscv64/build/nanokvm-server-4af32fc7032b9dde2f3efd4d917837eeca015a87/test/node_modules/.pnpm/nanoid@3.3.8/node_modules/nanoid/node_modules:/build/buildroot/output/cvitek_CV181X_glibc_riscv64/build/nanokvm-server-4af32fc7032b9dde2f3efd4d917837eeca015a87/test/node_modules/.pnpm/nanoid@3.3.8/node_modules:/build/buildroot/output/cvitek_CV181X_glibc_riscv64/build/nanokvm-server-4af32fc7032b9dde2f3efd4d917837eeca015a87/test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nanoid@3.3.8/node_modules/nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../../../../../nanoid@3.3.8/node_modules/nanoid/bin/nanoid.cjs" "$@"
fi
